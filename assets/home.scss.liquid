                        color: #555550;
                    }
                }

                .faq_list {
                    display: flex;
                    width: 100%;
                    flex-direction: column;

                    .faq_item {
                        background-color: #FFFFFF;
                        padding: 15px 51px 15px 20px;
                        position: relative;
                        display: flex;
                        align-items: center;
                        transition: all 0.5s ease;
                        border: 2px solid #F7F7F7;
                        cursor: pointer;

                        &:before {
                            content: "";
                            background-image: url({{ 'arrow_down.png' | asset_url }});
                            background-size: contain;
                            background-repeat: no-repeat;
                            background-position: center;
                            width: 18px;
                            height: 8px;
                            position: absolute;
                            right: 20px;
                            top: 42px;
                            transform: rotate(0deg);
                            transition: all 0.5s ease;
                        }

                        .faq_icon {
                            margin-right: 20px;
                            color: #16B9EE;
                            transition: all 0.5s ease;
                            margin-bottom: auto;
                        }

                        &:first-child {
                            .faq_content {
                                .faq_answer {
                                    display: block;
                                }
                            }
                        }

                        .faq_content {
                            .faq_question {
                                margin-top: 0;
                                margin-bottom: 5px;
                                font-weight: 600;
                                font-size: 16px;
                                line-height: 25.6px;
                                letter-spacing: 0.4px;
                                text-transform: uppercase;
                                color: #2B296B;
                            }

                            .faq_answer {
                                color: #2B296B;
                                font-weight: 400;
                                font-size: 14px;
                                line-height: 22.6px;
                                letter-spacing: 0.35px;
                                display: none;
                            }
                        }

                        &.active {
                            background-color: #B9E5FB;

                            &:before {
                                transform: rotate(180deg);
                            }

                            .faq_icon {
                                color: #2B296B;
                            }
                        }
                    }
                }
            }

            .right_content {
