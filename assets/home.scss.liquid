.header-sticky-wrapper {
    .site-header-sticky {
        height: auto !important;
    }

    .site-header {
        background-color: #FFFFFF;
        padding: 55px 0 7px;

        .header-item--icons {
            .site-nav {
                .site-nav__link {
                    color: #2B296B;
                    transition: all 0.5s ease;

                    &:hover {
                        color: #16b9ee;
                    }

                    .cart-link {
                        .cart-link__bubble--visible {
                            border: medium none;
                        }
                    }
                }
            }
        }

        .header-item--logo {
            .site-header__logo {
                margin: 0;

                a {
                    width: auto;

                    img {
                        height: 72px;
                        object-fit: contain;
                        max-width: 180px;
                    }
                }
            }
        }

        .site-nav {
            .site-nav__item {
                background-color: transparent !important;
                color: #050b2d;

                &.site-nav--has-dropdown:first-child {
                    background: #8dc63f !important;
                    color: #FFFFFF;
                }

                &:first-child {
                    >.site-nav__link {
                        color: #FFFFFF;

                        &:hover {
                            background: #8dc63f !important;
                            color: #FFFFFF !important;
                        }
                    }
                }

                .site-nav__dropdown {
                    .site-nav__dropdown-link {
                        transition: all 0.5s ease;

                        &:hover {
                            color: #16b9ee;
                        }
                    }
                }

                >.site-nav__link {
                    padding: 8.5px 11px;
                    transition: all 0.5s ease;
                    color: #050B2D;
                    font-size: 15px;
                    letter-spacing: 2.34px;
                    line-height: 25px;

                    &:hover {
                        background-color: transparent !important;
                        color: #050B2D !important;
                    }
                }

            }
        }

        .header_nav_with_pawpass {
            position: relative;
            display: flex;
            justify-content: center;
            width: 100%;

            .site-navigation {
                margin: 0;
            }

            .pawpass_logo {
                margin-left: 18px;
              	margin-top: -5px;

                .pawpass_logo_link {
                    display: inline-block;
                }
            }
        }
    }
}

.custom_slideshow {
    background-color: #16b9ee;
    padding: 68px 0 232px;

    .slick-dots {
        display: none;
    }

    &.hero-auto {
        height: auto;
    }

    .slideshow__slide {
        .hero__image-wrapper {
            max-width: 1500px;
            margin-left: auto;
            margin-right: auto;
            left: 50%;
            transform: translateX(-50%);

            &:before {
                opacity: 0;
            }

            .hero__image {
                min-width: 550px;
                margin-left: auto;
                width: calc(550 / 1286 * 100vw);
            }
        }

        .hero__text-wrap {
            position: relative;
            height: calc(383 / 1286 * 100vw);
            color: #fff;
            min-height: 383px;

            .hero__text-content {
                display: flex;
                align-items: center;
                height: 100%;
                padding-left: 42px;
                // vertical-align: auto;

                .hero__title {
                    font-family: 'MyriadPro-BoldIt';
                    // font-style: italic;
                    text-transform: lowercase;
                    letter-spacing: 0.81px;

                    sup {
                        text-transform: uppercase;
                        margin-left: 7px;
                        font-size: 30px;
                        top: -1em;
                    }
                }

                .hero__subtitle {
                    font-weight: 600;
                    font-size: 20px;
                    line-height: 28px;
                }

                .hero__link {
                    margin-top: 40px;

                    .btn {
                        letter-spacing: 3.9px;
                        font-size: 13px;
                        color: #fff;
                        font-family: Montserrat;
                        font-weight: 700;
                        outline: none;
                        padding: 15px 20px;
                    }
                }
            }
        }
    }
}

