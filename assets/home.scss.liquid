                    font-size: 24px;
                    line-height: 36.2px;
                    letter-spacing: 1.7px;
                    color: #FFFFFF;
                    margin-bottom: 20px;
                    text-transform: uppercase;
                    width: 400px;
                    text-align: center;
                }

                .natural_slider {
                    width: 510px;

                    .slick-arrow {
                        font-size: 0;
                        padding: 0;
                        margin: 0;
                        position: absolute;
                        top: 55%;
                        transform: translateY(-50%);
                        background-repeat: no-repeat;
                        background-size: contain;
                        background-position: center;
                        width: 39px;
                        height: 39px;
                        background-size: 11px;
                        border-radius: 100%;
                        background-color: transparent;
                        transition: all 0.5s ease;

                        &.slick-prev {
                            background-image: url({{ 'arrow_left.png' | asset_url }});
                            left: -50px;
                            transition: all 0.5s ease;
                        }

                        &.slick-next {
                            background-image: url({{ 'arrow_right.png' | asset_url }});
                            right: -50px;
                            transition: all 0.5s ease;
                        }

                        &:hover {
                            background-color: #F7F7F7;

                            &.slick-prev {
                                background-image: url({{ 'arrow_left_active.png' | asset_url }});
                            }

                            &.slick-next {
                                background-image: url({{ 'arrow_right_active.png' | asset_url }});
                            }
                        }
                    }

                    .natural_slider_item_inner {
