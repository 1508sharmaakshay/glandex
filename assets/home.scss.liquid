.custom_collection_tab_section {
    .page-width {
        max-width: 1230px;
        padding-left: 30px;
        padding-right: 30px;
    }

    .custom_collection_tab_section_inner {
        .section-header__title {
            font-size: 32px;
            letter-spacing: 1.6px;
            line-height: 39px;
            font-weight: 600;
            color: #2B296B;
            text-transform: none;
        }

        .custom_collection_tab_list {
            .custom_collection_tabs {
                display: flex;
                width: 100%;

                .custom_collection_tab_item {
                    flex: auto;
                    width: calc(100% / 6);
                    padding: 0 5px 20px;
                    position: relative;

                    &:after {
                        content: "";
                        width: 40px;
                        height: 40px;
                        position: absolute;
                        left: 50%;
                        transform: translateX(-50%);
                        bottom: 0;
                        background-color: #16B9EE;
                        border-radius: 100%;
                        transition: all 0.5s ease;
                        opacity: 0;
                    }

                    &:hover {
                        .item_data {
                            &.desktop_only {
                                border-color: #16B9EE;

                                // .item_icon {
                                //     .normal {
                                //         opacity: 0;
                                //     }

                                //     .hover {
                                //         opacity: 1;
                                //     }
                                // }

                                // .item_heading {
                                //     color: #FFFFFF;
                                // }
                            }
                        }
                    }

                    &.active {
                        &:after {
                            opacity: 1;
                        }

                        .item_data {
                            &.desktop_only {
                                background-color: #16B9EE;
                                border-color: #16B9EE;

                                .item_icon {
                                    .normal {
                                        opacity: 0;
                                    }

                                    .hover {
                                        opacity: 1;
                                    }
                                }

                                .item_heading {
                                    color: #FFFFFF;
                                }
                            }
                        }
                    }

                    .item_data {
                        cursor: pointer;

                        &.mobile_only {
                            display: none;
                        }

                        &.desktop_only {
                            min-height: 146px;
                            padding: 20px 10px;
                            background-color: #FFFFFF;
                            border-radius: 5px;
                            box-shadow: 0 0 8px 1px RGBA(0, 0, 0, 0.1);
                            display: flex;
                            flex-direction: column;
                            align-items: center;
                            transition: all 0.5s ease;
                            border: 2px solid #FFFFFF;

                            .item_icon {
                                width: 45px;
                                height: 45px;
                                position: relative;
                                margin-bottom: 10px;

                                .normal {
                                    position: relative;
                                    max-width: 100%;
                                    opacity: 1;
                                    transition: all 0.5s ease;
                                }

                                .hover {
                                    position: absolute;
                                    opacity: 0;
                                    left: 0;
                                    top: 0;
                                    transition: all 0.5s ease;
                                }
                            }

                            .item_heading {
                                font-size: 16px;
                                line-height: 22px;
                                color: #050B2D;
                                font-weight: 600;
                                text-align: center;
                                transition: all 0.5s ease;
                            }
                        }
                    }
                }
            }

            .custom_collection_tabs_content {
                width: calc(100% - 100px);
                margin: 56px auto 0;

                .custom_collection_tab_item_content {
                    display: none;
                    width: 100%;
                    justify-content: space-between;

                    &.active {
                        display: flex;
                    }

                    .left_content {
                        max-width: 500px;
                        margin-top: 29px;

                        .heading {
                            font-weight: 600;
                            font-size: 32px;
                            letter-spacing: 2.9px;
                            line-height: 39px;
                            color: #2B296B;
                            text-transform: uppercase;
                            margin-bottom: 15px;
                            max-width: 80%;
                        }

                        .sub_heading {
                            font-size: 20px;
                            line-height: 28px;
                            font-weight: 600;
                            color: #555550;
                            margin-bottom: 30px;
                        }

                        .content {
                            font-weight: 500;
                            letter-spacing: 0.4px;
                            line-height: 20px;
                            color: #555550;
                            margin-bottom: 40px;

                            p {
                                font-weight: 500;
                                letter-spacing: 0.4px;
                                line-height: 20px;
                                color: #555550;
                            }

                            ul {
                                list-style: none;
                                padding-left: 0;
                                margin: 0;

                                li {
                                    margin: 0 0 15px;
                                    position: relative;
                                    padding-left: 16px;
                                    font-weight: 500;
                                    letter-spacing: 0.4px;
                                    line-height: 20px;
                                    color: #555550;

                                    &:before {
                                        content: "";
                                        width: 6px;
                                        height: 6px;
                                        background-color: #2B296B;
                                        border-radius: 100%;
                                        position: absolute;
                                        left: 0;
                                        top: 6px;
                                    }

                                    &:last-child {
                                        margin-bottom: 0;
                                    }
                                }
                            }
                        }

                        .cta {
                            .btn {
                                padding: 13px 20px 12px;
                                font-size: 13px;
                                letter-spacing: 3.9px;
                                color: #FFFFFF;
                                text-transform: uppercase;
                                background-color: #8DC63F;
                                font-family: Montserrat;
                                font-weight: 700;
                            }
                        }
                    }
                }
            }
        }
    }
}

